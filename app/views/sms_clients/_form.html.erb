<%= simple_form_for @sms_client, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :ClientName %>
  <%= f.input :NationalID %>
  <%= f.input :SeasonName %>
  <%= f.input :EnrollmentDate %>
  <%= f.input :Dropped %>
  <%= f.input :DroppedDate %>
  <%= f.input :Facilitator %>
  <%= f.input :SeasonDataEntryDate %>
  <%= f.input :SeasonID %>
  <%= f.input :Deceased %>
  <%= f.input :GroupName %>
  <%= f.input :SiteName %>
  <%= f.input :SectorName %>
  <%= f.input :DistrictName %>
  <%= f.input :GovDistrictName %>
  <%= f.input :ClientID %>
  <%= f.input :GroupID %>
  <%= f.input :SiteID %>
  <%= f.input :SectorID %>
  <%= f.input :DistrictID %>
  <%= f.input :GovDistrictID %>
  <%= f.input :FieldOfficer %>
  <%= f.input :FieldManager %>
  <%= f.input :FOID %>
  <%= f.input :FMID %>
  <%= f.input :GroupFirstSeason %>
  <%= f.input :SiteFirstSeason %>
  <%= f.input :StockID %>
  <%= f.input :LandWaterHarvest %>
  <%= f.input :GroupActive %>
  <%= f.input :SiteActive %>
  <%= f.input :FirstSeason %>
  <%= f.input :TotalCredit %>
  <%= f.input :RepaymentTotal %>
  <%= f.input :Service %>
  <%= f.input :Delivery %>
