  <%= f.input :SeasonClientID %>
  <%= f.input :LastRepayment %>
  <%= f.input :ExtendedClientID %>
  <%= f.input :ClientPhoneNumber %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                sms_clients_path, :class => 'btn' %>
  </div>
<% end %>
